{
  "version": "7.0.0-beta3",
  "description": "Command line utilities for Entity Framework.",
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "dependencies": {
    "EntityFramework.Relational": "7.0.0-beta3",
    "Microsoft.Framework.DependencyInjection": "1.0.0-beta3"
  },
  "code": [
    "**\\*.cs",
    "..\\Shared\\*.cs"
  ],
  "exclude": "tools\\Handlers.cs",
  "frameworks": {
    "net451": {},
    "aspnet50": {
      "dependencies": {
        "Microsoft.AspNet.Hosting": "1.0.0-beta3",
        "Microsoft.Framework.CommandLineUtils": {
          "version": "1.0.0-beta3",
          "type": "build"
        }
      }
    },
    "aspnetcore50": {
      "dependencies": {
        "Microsoft.AspNet.Hosting": "1.0.0-beta3",
        "Microsoft.Framework.CommandLineUtils": {
          "version": "1.0.0-beta3",
          "type": "build"
        }
      }
    }
  },
  "commands": {
    "ef": "EntityFramework.Commands"
  }
}